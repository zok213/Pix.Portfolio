{
    "outputDirectory": "Landing_page/dist/spa",
    "trailingSlash": true,
    "redirects": [
        {
            "source": "/",
            "destination": "/talent",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/talent",
            "destination": "/talent/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}